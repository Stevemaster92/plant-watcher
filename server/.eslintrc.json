{
    "root": true,
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "indent": ["error", 4],
        "object-curly-spacing": 0,
        "new-cap": 0,
        "require-jsdoc": 0,
        "no-prototype-builtins": 0,
        "no-unused-vars": 1,
        "linebreak-style": "off",
        "max-len": ["warn", 120]
    }
}
